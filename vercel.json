{
  "version": 2,
  "builds": [
    {
      "src": "core/asgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "public,max-age=31536000,immutable" },
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "headers": { "cache-control": "public,max-age=31536000,immutable" },
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "core/asgi.py"
    }
  ]
}
