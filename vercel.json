{
  "builds": [
    { "src": "core/asgi.py", "use": "@vercel/python" }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "core/asgi.py"
    }
  ]
}
